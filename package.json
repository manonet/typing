  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache",
    "develop": "gatsby develop",
    "localize": "sh ./scripts/poedit.sh",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --config ./jest.config.js",
    "test:unit": "NODE_PATH=src jest --config ./jest.config.js",
    "test:unit:watch": "NODE_PATH=src jest --watch --config ./jest.config.watch.js",
    "lint": "tsc && npm run lint:styles && npm run lint:eslint",
    "lint:eslint": "eslint src --max-warnings 20 --ext tsx --ext ts",
    "lint:styles": "stylelint '**/*.less' --syntax less",
    "esfix": "eslint src --max-warnings 20 --ext tsx --ext ts --fix",
    "tsc": "tsc",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run localize"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manonet/typing"
  },
  "bugs": {
    "url": "https://github.com/manonet/typing/issues"
