  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf .cache",
    "develop": "gatsby develop",
    "localize": "sh ./scripts/poedit.sh",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --config ./jest.config.js",
    "test:unit": "NODE_PATH=src jest --config ./jest.config.js",
    "test:unit:watch": "NODE_PATH=src jest --watch --config ./jest.config.watch.js",
    "lint": "npm run lint:styles",
    "lint:eslint": "eslint ./src --max-warnings 20 --ext jsx --ext js",
    "lint:styles": "stylelint '**/*.scss' --syntax scss",
    "esfix": "eslint ./src --max-warnings 20 --ext jsx --ext js --fix",
    "tsc": "tsc",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx run lint",
      "pre-push": "npx localize"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manonet/typing"
  },
  "bugs": {
    "url": "https://github.com/manonet/typing/issues"
  }
